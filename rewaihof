while wait() do
    if game.Players.LocalPlayer.Character.HumanoidRootPart then
        if game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Y < -20 then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(56,-5,-33)
            warn("saved!")
    else
        wait()
        end
    end
end
